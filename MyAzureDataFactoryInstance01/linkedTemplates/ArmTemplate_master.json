{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MyAzureDataFactoryInstance01"
		},
		"LS_AzureSQLDb_Connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSQLDb_Connection'"
		},
		"LS_Sink_Zip_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Sink_Zip'"
		},
		"LS_Source_Connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Source_Connection'"
		},
		"LS_Source_Zip_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Source_Zip'"
		},
		"LS_Target_Connection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Target_Connection'"
		},
		"LS_HttpServer_Connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"LS_Sink_Zip_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://targetdlgen2storageacc.dfs.core.windows.net/"
		},
		"LS_Target_Connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://targetdlgen2storageacc.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "MyAzureDataFactoryInstance01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureSQLDb_Connection_connectionString": {
						"value": "[parameters('LS_AzureSQLDb_Connection_connectionString')]"
					},
					"LS_Sink_Zip_accountKey": {
						"value": "[parameters('LS_Sink_Zip_accountKey')]"
					},
					"LS_Source_Connection_connectionString": {
						"value": "[parameters('LS_Source_Connection_connectionString')]"
					},
					"LS_Source_Zip_connectionString": {
						"value": "[parameters('LS_Source_Zip_connectionString')]"
					},
					"LS_Target_Connection_accountKey": {
						"value": "[parameters('LS_Target_Connection_accountKey')]"
					},
					"LS_HttpServer_Connection_properties_typeProperties_url": {
						"value": "[parameters('LS_HttpServer_Connection_properties_typeProperties_url')]"
					},
					"LS_Sink_Zip_properties_typeProperties_url": {
						"value": "[parameters('LS_Sink_Zip_properties_typeProperties_url')]"
					},
					"LS_Target_Connection_properties_typeProperties_url": {
						"value": "[parameters('LS_Target_Connection_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "MyAzureDataFactoryInstance01_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'MyAzureDataFactoryInstance01_ArmTemplate_0')]"
			]
		}
	]
}